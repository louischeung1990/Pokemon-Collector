<%- include('../partials/header') %>

<main id="user-view">
<div id="userInfo">
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>GoogleId</th>
        <th>Custom Name</th>
        <th>Date Created</th>
        <th>Avatar</th>
      </tr>
    </thead>
    <tbody>
      <% account.forEach(function(el) { %>
        <tr>
          <td><%= el.name %></td>
          <td><%= el.email %></td>
          <td><%= el.googleId %></td>
          <td><%= el.customName %></td>
          <td><%= el.dateCreated %></td>
          <td><%= el.avatar %></td>
          <td>
            <form action="/users/<%= el.id %>?_method=DELETE" method="POST">
            <button type="submit">Delete</button>
            </form>
          </td>
        </tr>
        <% }); %>
      </tbody>
    </table>
</div>
</main>

<%- include('../partials/footer') %>